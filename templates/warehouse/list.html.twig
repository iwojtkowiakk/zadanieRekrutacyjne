{% extends 'base.html.twig' %}

{% block body %}
<h1 class="h3 mb-3 font-weight-normal">Magazyn: <b>{{ warehouse.name }}</b></h1>


<table class="table table-dark table-striped">
    <thead>
    <tr>
        <th>Product Name</th>
        <th>Quantity</th>
    </tr>
    </thead>
    <tbody>
    {% for product in products %}
        <tr>
            <td>{{ product.name }}</td>
            <td>{{ product.quantity }}</td>
        </tr>
    {% endfor %}
    </tbody>

    {% endblock %}
